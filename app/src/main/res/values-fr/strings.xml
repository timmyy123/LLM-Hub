    <string name="about_llm_hub">À propos de LLM Hub</string>
    <string name="about_description">LLM Hub est une application Android open-source qui apporte la puissance des Large Language Models directement sur votre appareil mobile.</string>
    <string name="key_features">Fonctionnalités Clés</string>
    <string name="feature_multiple_models"> Modèles LLM Multiples : Gemma-3, Llama-3.2, Phi-4, et Gemma-3n</string>
    <string name="feature_on_device"> Traitement sur Appareil : Confidentialité complète, aucun internet requis</string>
    <string name="feature_vision_support"> Support Vision : Modèles multimodaux qui comprennent les images</string>
    <string name="feature_gpu_acceleration"> Accélération GPU : Performance optimisée sur les appareils compatibles</string>
